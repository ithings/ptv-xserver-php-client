{
    "name": "ithings/ptv-xserver-php-client",
    "description": "PTV xServer API PHP client generated with swagger",
    "keywords": [
        "swagger",
        "php",
        "ptv",
        "api"
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/"}
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/"}
    }
}
